{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema-preview",
  "alwaysOnTop": false,
  "alwaysShowNotificationIcon": false,
  "confirmCloseAllTabs": true,
  "copyFormatting": "all",
  "copyOnSelect": true,
  "defaultProfile": "{1d2d9f1c-0192-5285-8115-38593c2c526e}",
  "disableAnimations": false,
  "focusFollowMouse": false,
  "launchMode": "focus",
  "minimizeToNotificationArea": false,
  "showTabsInTitlebar": true,
  "startOnUserLogin": true,
  "theme": "KDark",
  "trimBlockSelection": true,
  "trimPaste": true,
  "wordDelimiters": " /\\()\"'-.,:;<>~!@#$%^&*|+=[]{}~?\u2502",
  "actions": [
    {
      "id": "User.sendInput.ControlI",
      "command": { "action": "sendInput", "input": "\u001b[105;5u" }
    },
    {
      "id": "User.sendInput.ControlBackspace",
      "command": { "action": "sendInput", "input": "\u001b[8;5u" }
    },
    {
      "id": "User.sendInput.ShiftEnter",
      "command": { "action": "sendInput", "input": "\u001b[13;2u" }
    },
    {
      "id": "User.sendInput.ControlSpace",
      "command": { "action": "sendInput", "input": "\u001b[64;5u" }
    },
    {
      "id": "User.sendInput.ControlShiftEnter",
      "command": { "action": "sendInput", "input": "\u001b[13;6u" }
    },
    {
      "id": "User.sendInput.ControlEnter",
      "command": { "action": "sendInput", "input": "\u001b[13;5u" }
    }
  ],
  "keybindings": [
    // disabled keybinds
    { "id": null, "keys": "alt+enter" },
    { "id": null, "keys": "ctrl+c" },
    { "id": null, "keys": "ctrl+v" },
    // clipboard
    { "id": "Terminal.CopyToClipboard", "keys": "ctrl+shift+c" },
    { "id": "Terminal.PasteFromClipboard", "keys": "ctrl+shift+v" },
    // text manipulation
    { "id": "Terminal.ResetFontSize", "keys": "ctrl+shift+`" },
    { "id": "Terminal.IncreaseFontSize", "keys": "ctrl+shift+delete" },
    { "id": "Terminal.DecreaseFontSize", "keys": "ctrl+shift+backspace" },
    { "id": "Terminal.ClearBuffer", "keys": "ctrl+shift+g" },
    // app management
    { "id": "Terminal.ToggleFocusMode", "keys": "ctrl+;" },
    { "id": "Terminal.ToggleCommandPalette", "keys": "ctrl+shift+p" },
    { "id": "Terminal.OpenSettingsUI", "keys": "ctrl+shift+s" },
    // passthrough
    { "id": "User.sendInput.ControlShiftEnter", "keys": "ctrl+shift+enter" },
    { "id": "User.sendInput.ControlBackspace", "keys": "ctrl+backspace" },
    { "id": "User.sendInput.ControlEnter", "keys": "ctrl+enter" },
    // { "id": "User.sendInput.ControlSpace", "keys": "ctrl+space" },
    { "id": "User.sendInput.ControlI", "keys": "ctrl+i" },
    { "id": "User.sendInput.ShiftEnter", "keys": "shift+enter" },
    // window/pane management
    { "id": "Terminal.DuplicatePaneRight", "keys": "ctrl+shift+/" },
    { "id": "Terminal.DuplicatePaneDown", "keys": "ctrl+shift+minus" },
    { "id": "Terminal.MoveFocusDown", "keys": "ctrl+shift+j" },
    { "id": "Terminal.MoveFocusUp", "keys": "ctrl+shift+k" },
    { "id": "Terminal.MoveFocusLeft", "keys": "ctrl+shift+h" },
    { "id": "Terminal.MoveFocusRight", "keys": "ctrl+shift+l" },
    { "id": "Terminal.ResizePaneDown", "keys": "alt+shift+j" },
    { "id": "Terminal.ResizePaneUp", "keys": "alt+shift+k" },
    { "id": "Terminal.ResizePaneLeft", "keys": "alt+shift+h" },
    { "id": "Terminal.ResizePaneRight", "keys": "alt+shift+l" },
    { "id": "Terminal.TogglePaneZoom", "keys": "ctrl+shift+z" },
    // tab navigation
    { "id": "Terminal.PrevTab", "keys": "ctrl+shift+d" },
    { "id": "Terminal.NextTab", "keys": "ctrl+shift+f" },
    { "id": "Terminal.CloseTab", "keys": "ctrl+shift+w" }
  ],
